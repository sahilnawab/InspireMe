@model IEnumerable<InspireMe.Models.Quote>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<div class="container">
    <div class="row row-cols-1 row-cols-md-3 g-4">
        @foreach (var item in Model)
        {
            <div class="col">
                @await Html.PartialAsync("_QuoteCard", item)
            </div>
        }
    </div>
</div>
